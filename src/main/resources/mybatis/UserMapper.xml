<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.forum.model.mapper.UserMapper">
	<insert id="insertUser" parameterType="Information">
		 insert into information(username,email,mobile,password)
		 values(#{username},#{email},#{mobile},#{password})
	</insert>






</mapper>